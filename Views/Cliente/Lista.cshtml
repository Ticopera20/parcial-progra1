@model List<Cliente>

<h1>Lista de Cliente</h1>

@if (Model.Count == 0) {
    <div class="alert alert-warning text-center">NO HAY NADA.</div>
} else {
    <ol>
    @foreach (var c in Model) {
        <li class="d-flex">
            <div>@c.Nombre</div>
            
            <div>@c.Email</div>

            <div>@c.TipoCuenta</div>

            <div>@c.SaldoInicial</div>


            
        </li>
    }
    </ol>
}